class Person(object):

  	
  	def __init__(self, name, idnumber):
          self.name = name
          self.idnumber = idnumber

  	def display(self):
          print(self.name)
          print(self.idnumber)


   b 
obj = Person("Khalid", 257853)


print("Name: ", obj.name)
print("Id: ", obj.idnumber)



obj.display()

